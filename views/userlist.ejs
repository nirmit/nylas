<div class="abc">       
<section>
<a href="/add/user"><button type="button" class="btn btn-primary" >Add User</button></a>
<div class="inner-block usr_list_div">
<h1>All User</h1>
<div class="frm">
     <div class="table-responsive left width100">
<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>accessToken</th>    
    <th>Action</th>    
  </tr>
  </thead>
  <tbody>
  <% if (userlist.length > 0) { %>
    <% for(var i=0; i < userlist.length; i++) { %>
      <tr>
        <td><%= userlist[i].firstname %></td>
        <td><%= userlist[i].lastname %></td>
        <td><%= userlist[i].email %></td>
        <td><%= userlist[i].phone %></td>
         <td><%= userlist[i].accessToken %></td>         
        <td><a href="<%= sitelink %>/edituser/<%= userlist[i].id %>"><span class="fa fa-edit"></span></a>&nbsp;<a href="<%= sitelink %>/removeuser/<%= userlist[i].id %>"><span class="fa fa-remove"></span></a></td> 
      </tr> 
    <% } %>
  <% }else{ %>
    <tr><td colspan="4" style="text-align:center;">No Users Found.</td></tr>
  <% } %>
  </tbody>
  </table>
      
 </div>
 </div>
</section>

<script type="text/javascript">
function switch_user(id,role){
  
  $.ajax({
    type: 'post',
    url: '/switch_user',
    data: 'id='+id+'&role='+role,
    complete: function(){
          location.href = location.href;
    }
  });
  
}
</script>


